{
  "name": "bcflame-backend",
  "private": true,
  "version": "0.1.0",
  "description": "BC Flame Premium Portal - Strapi Backend",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build && npm run copy:templates",
    "copy:templates": "mkdir -p dist/src/templates/html && cp -r src/templates/html/* dist/src/templates/html/",
    "strapi": "strapi",
    "seed": "ts-node database/seed.ts",
    "types:generate": "strapi ts:generate-types",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@strapi/plugin-i18n": "4.16.2",
    "@strapi/plugin-users-permissions": "4.16.2",
    "@strapi/strapi": "4.16.2",
    "@types/socket.io": "^3.0.1",
    "archiver": "^7.0.1",
    "better-sqlite3": "9.2.2",
    "dotenv": "^17.2.3",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "pg": "8.11.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^5.2.0",
    "resend": "^6.9.1",
    "socket.io": "^4.8.3",
    "styled-components": "^5.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@strapi/typescript-utils": "4.16.2",
    "@types/archiver": "^6.0.0",
    "@types/node": "20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.0",
    "@vitest/coverage-v8": "^1.1.1",
    "ts-node": "^10.9.2",
    "vitest": "^1.1.1"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  }
}
